PORT=4000
NODE_ENV=development
CORS_ORIGIN=http://localhost:5173

# IMPORTANT: Do not expose the SERVICE_ROLE_KEY to the Frontend or Vercel config.
# This key allows the Node backend to bypass Row Level Security.
SUPABASE_URL=your_project_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# This anon key is only needed if verifying tokens locally or decoding headers via auth client setup.
SUPABASE_ANON_KEY=your_anon_key
MAX_BULK_IMPORT=500
MAX_LICENSE_SIZE_MB=5
PYTHON_COMMAND=python3
MAX_IMAGE_SIZE_MB=1.5
REMOVE_BG_ENABLED=true
TZ=America/Caracas
